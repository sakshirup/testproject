            android:parentActivityName=".Main2Activity" />
        <activity
            android:name=".Create_New_Account"
            android:parentActivityName=".Main2Activity" />
        <activity
            android:name=".Starter"
            android:parentActivityName=".Home" />
        <activity android:name=".sms" />
        <activity
            android:name=".location"
            android:parentActivityName=".Home" />
        <activity
            android:name=".about_us"
            android:parentActivityName=".Home" />
        <activity
            android:name=".feedback"
            android:parentActivityName=".Home" />
        <activity
            android:name=".Dessert"
            android:parentActivityName=".Home" />
        <activity android:name=".Salad" />
        <activity android:name=".main_course" />
        <activity android:name=".rate" />
        <activity android:name=".timer" />
        <activity android:name=".drink"></activity>
